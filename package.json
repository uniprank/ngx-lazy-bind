{
    "name": "ngx-lazy-bind",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --project=TestCases --host=0.0.0.0",
        "build": "ng build NgxLazyBind",
        "build:watch": "ng build NgxLazyBind --watch",
        "build:package": "ng build NgxLazyBind",
        "build:testcases": "ng build --project=TestCases --prod",
        "test": "ng test NgxLazyBind --watch=false --coverage",
        "test:watch": "ng test NgxLazyBind --watch=true --coverage",
        "coverage": "http-server -c-1 -o -p 9875 ./coverage/ngx-lazy-bind",
        "release:bug": "npm version patch --prefix ./library/ngx-lazy-bind",
        "release:feature": "npm version minor --prefix ./library/ngx-lazy-bind",
        "publish": "cd dist/ngx-lazy-bind && npm publish --access public",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "search:port:pid": "lsof -n -i4TCP:8080"
    },
    "license": "MIT",
    "private": false,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "core-js": "^2.5.4",
        "ngx-markdown": "^7.1.5",
        "rxjs": "~6.3.3",
        "stickyfilljs": "^2.1.0",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-builders/jest": "^7.4.2",
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular-devkit/build-ng-packagr": "~0.13.0",
        "@angular/cli": "~7.3.8",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "^24.0.12",
        "@types/node": "^8.9.5",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^24.7.1",
        "jest-junit": "^6.4.0",
        "jest-junit-reporter": "^1.1.0",
        "ng-packagr": "^4.2.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": ">=0.34.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2"
    }
}
