{
  "name": "ngx-lazy-bind",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=TestCases --host=0.0.0.0",
    "build": "ng build NgxLazyBind",
    "build:watch": "ng build NgxLazyBind --watch",
    "build:package": "ng build NgxLazyBind --prod",
    "build:testcases": "ng build --project=TestCases --prod",
    "test": "ng test NgxLazyBind --watch=false",
    "test:travis": "ng test NgxLazyBind --watch=false",
    "test:watch": "ng test NgxLazyBind --watch",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage/ngx-lazy-bind",
    "release:bug": "npm version patch --prefix ./library/ngx-lazy-bind",
    "release:feature": "npm version minor --prefix ./library/ngx-lazy-bind",
    "publish": "cd dist/ngx-lazy-bind && npm publish --access public",
    "lint": "ng lint NgxLazyBind",
    "search:port:pid": "lsof -n -i4TCP:8080"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "core-js": "^2.5.4",
    "ngx-markdown": "^10.1.1",
    "rxjs": "~6.6.2",
    "stickyfilljs": "^2.1.0",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.4.2",
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "~10.0.14",
    "@angular/language-service": "~10.0.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "fstream": "^1.0.12",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "jest-junit-reporter": "^1.1.0",
    "ng-packagr": "^10.1.0",
    "node-sass": "^4.14.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.9.7"
  }
